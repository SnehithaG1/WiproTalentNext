import java.util.*;

public class Factorial{
	public static int Fact(int inp){
		int fact=1;
		while(inp>0){
			fact*=inp;
			inp--;
		}
		return fact;
		
	}
	
public static void main(String[] args){
     Scanner st = new Scanner(System.in);
	   int inp=st.nextInt();
     System.out.println(Fact(inp));
     st.close();
	
   }
}
