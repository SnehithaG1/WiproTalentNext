import java.io.*;
import java.math.*;
import java.util.*;

class LastDigit{
public static int LastDigitOfNumber(int input){
    return(abs(input)%10);
    }
public static void main(String[] args){
     Scanner s = new Scanner(System.in);
     int n = s.nextInt();
     System.out.println(LastDigitOfNumber(n));
     }
}
